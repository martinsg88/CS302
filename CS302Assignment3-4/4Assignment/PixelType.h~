#ifndef PIXELTYPE_H
#define PIXELTYPE_H

#include <cstdlib>

#include "NodeType.h"

class PixelType{
public:
  int i;
  int j;
  PixelType() {i = 0; j = 0; };  // In-line Constructor
  PixelType& operator= (PixelType&);  ~PixelType() {i = 0; j = 0; }; // In-line Destructor};
#endif

//Default Constructor-----------------------------------------------------------
//{
//		In-line
//}
//Overlaoded = Operator for int-------------------------------------------------
PixelType& PixelType::operator= (PixelType& object)
{
	i = object.i;
	j = object.j;

//	return(*this);	
}
//Default Destructor------------------------------------------------------------
//{
//		In-line
//}
